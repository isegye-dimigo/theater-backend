		"dotenv": "^16.3.1",
		"fastify": "^4.24.3",
		"fluent-json-schema": "^4.1.2",
		"ioredis": "^5.3.2",
		"mariadb": "^3.2.2",
		"smtp-channel": "^0.2.4",
		"tsconfig-paths": "^4.2.0"
	},
	"devDependencies": {
		"@types/node": "^18.18.6",
