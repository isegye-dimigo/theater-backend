	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.391.0",
		"@fastify/multipart": "^7.7.3",
		"@prisma/client": "^5.2.0",
		"dotenv": "^16.0.3",
		"fastify": "^4.17.0",
		"fluent-json-schema": "^4.1.0",
		"ioredis": "^5.3.2",
		"mariadb": "^3.1.2",
		"smtp-channel": "^0.2.4",
		"tsconfig-paths": "^4.2.0"
	},
	"devDependencies": {
		"@types/ioredis": "^5.0.0",
		"@types/node": "^18.16.12",
		"prisma": "^5.2.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.5"
	},
	"engines": {
		"node": ">= 18.0.0"
	}
